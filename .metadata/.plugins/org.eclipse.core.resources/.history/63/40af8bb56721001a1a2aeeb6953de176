package Array;

public class Demo3 {
	public static void main(String[] args) {
		int[] arr = {21,34,2,71,12,66,3};
		int result = getMax(arr);
		System.out.println(result);
	}
	
	//
	public static int getMax(int[] arr) {
		int Max = arr[0];
		for(int x=1;x<arr.length-1;x++) {
			if(arr[x]>Max) {
				Max = arr[x];
			}
		}
		return Max;
	}
}
