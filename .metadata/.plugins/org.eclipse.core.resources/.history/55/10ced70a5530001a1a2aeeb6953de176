package duotai;

class Person {
	public Person() {
	}

	public void eat() {
		System.out.println("吃饭");
	}
}

class SouthPerson extends Person {
	public SouthPerson() {
	}

	public void eat() {
		System.out.println("炒菜，吃米饭");
	}
}

class NorthPerson extends Person {
	public NorthPerson() {
	}

	public void eat() {

	}
}

public class DuotaiDemo {

}
