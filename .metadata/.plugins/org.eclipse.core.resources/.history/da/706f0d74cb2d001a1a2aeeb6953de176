package test2;

class Employee {
	private String name;
	private String eid;
	private int age;
	
	public Employee() {
		
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public void setEid(String eid) {
		this.eid = eid;
	}
	
	public String geteid() {
		return eid;
	}
	
	public void setAge(int age) {
		this.age = age;
	}
	
	public int getAge() {
		return age;
	}
	
	public void show() {
		System.out.println("姓名:"+name+"工号："+eid+"年龄:"+age);
	}
	
}

public class EmployeeDemo {

}
